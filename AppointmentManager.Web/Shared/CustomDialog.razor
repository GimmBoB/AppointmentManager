<MudDialog>
    <DialogContent>
        <MudText>@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">@CancelButtonText</MudButton>
        <MudButton Color="@Color" Variant="Variant.Text" OnClick="Submit">@SubmitButtonText</MudButton>
    </DialogActions>
</MudDialog>